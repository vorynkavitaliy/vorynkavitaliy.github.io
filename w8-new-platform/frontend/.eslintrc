{
  "parser": "babel-eslint",
  "rules": {
    "max-len": [1,1200,2,{"ignoreComments": true}],
    "no-console":[1],
    "react/jsx-uses-vars": [2],
    "no-loop-func":[1],
    "no-unused-vars": "warn",
    "indent": [
      "error",
      "tab"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "eslint:recommended"
  ]
}
